{
  "name": "@repo/erebus-sdk",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:types": "rm -rf dist/types",
    "build:js": "bun run build.config.ts",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly",
    "build:types:check": "tsc --project tsconfig.json --noEmit",
    "build": "bun run clean && bun run build:js && bun run build:types",
    "build:dev": "bun run build:js",
    "format": "bunx prettier . --write",
    "prepublishOnly": "bun run build",
    "test": "vitest run",
    "test:react": "vitest run --config vitest.jsdom.config.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
